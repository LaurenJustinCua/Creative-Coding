function setup() {
  // Create the canvas
  createCanvas(720, 400);
  background(200);

  fill(204, 101, 135, 100);
  stroke(100, 63, 120);

  rect(120, 120, 50, 50);

  ellipse(120, 240, 80, 50);

  triangle(120, 100, 320, 100, 310, 80);
  
  rect(50, 120, 50, 50);
  
  translate(580, 200);
  noStroke();
  for (let i = 0; i < 10; i ++) {
    rotate(PI/5);
  }
}
